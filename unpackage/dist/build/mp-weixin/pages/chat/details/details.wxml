<view><cu-custom vue-id="1" bgColor="bg-red" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">{{chattitle}}</view></cu-custom><view class="top"><view class="blessing">{{''+packet.blessing+" "+packet.dot+'点'}}</view><view class="money">{{''+packet.money+''}}</view><view class="face" style="{{'border-radius:'+(radius)+';'}}"><image src="{{face}}"></image></view><view class="username">{{''+packet.username+'的红包'}}</view></view><view class="info">{{'已领取'+packet.receivedNumber+"/"+packet.SumNumber+"个，共"+packet.receivedMoney+"/"+packet.SumMoney+'元'}}</view><view class="list"><block wx:for="{{packet.receivedList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="row"><view class="left"><image src="{{row.face}}"></image></view><view class="right"><view class="r1"><view class="username">{{''+row.username+''}}</view><view class="money">{{''+row.money+'元'}}</view></view><view class="r2"><view class="time">{{''+row.time+''}}</view><block wx:if="{{row.islucky}}"><view class="lucky">手气王</view></block></view></view></view></block></view></view>